	file = {Nitschke e.a. - 2020 - Efficient trajectory optimization for curved runni.pdf:/Users/j.t.heinen/Zotero/storage/JQMLIEU3/Nitschke e.a. - 2020 - Efficient trajectory optimization for curved runni.pdf:application/pdf},
