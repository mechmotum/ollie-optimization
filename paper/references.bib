@article{frederick_biomechanics_2006,
	title = {Biomechanics of {Skateboarding}: {Kinetics} of the {Ollie}},
	volume = {22},
	issn = {1065-8483, 1543-2688},
	shorttitle = {Biomechanics of {Skateboarding}},
	url = {https://journals.humankinetics.com/view/journals/jab/22/1/article-p33.xml},
	doi = {10.1123/jab.22.1.33},
	abstract = {Seven top amateur or professional skateboarders (BW = 713 N ± 83 N) performed Ollie maneuvers onto and off an elevated wooden platform (45.7 cm high). We recorded ground reaction force (GRF) data for three Ollie Up (OU) and Ollie Down (OD) trials per participant. The vertical GRF (VGRF) during the OU has a characteristic propulsive peak (M = 2.22 body weight [BW] ± 0.22) resulting from rapidly rotating the tail of the board into the ground to propel the skater and board up and forward. The anterior-posterior (A-P) GRF also shows a pronounced peak (M = 0.05 ± 0.01 BW) corresponding with this propulsive VGRF peak. The initial phase of landing in the OD shows an impact peak in VGRF rising during the first 30 to 80 ms to a mean of 4.74 ± 0.46 BW. These impact peaks are higher than expected given the relatively short drop of 45.7 cm and crouched body position. But we observed that our participants intentionally affected a firm landing to stabilize the landing position; and the Ollie off the platform raised the center of mass, also contributing to higher forces.},
	language = {en},
	number = {1},
	urldate = {2022-01-09},
	journal = {Journal of Applied Biomechanics},
	author = {Frederick, Edward C. and Determan, Jeremy J. and Whittlesey, Saunders N. and Hamill, Joseph},
	month = feb,
	year = {2006},
	pages = {33--40},
	file = {Frederick e.a. - 2006 - Biomechanics of Skateboarding Kinetics of the Oll.pdf:/Users/j.t.heinen/Zotero/storage/IMB4T36G/Frederick e.a. - 2006 - Biomechanics of Skateboarding Kinetics of the Oll.pdf:application/pdf},
}

@article{shield_contact-implicit_2022,
	title = {Contact-{Implicit} {Direct} {Collocation} {With} a {Discontinuous} {Velocity} {State}},
	volume = {7},
	issn = {2377-3766},
	doi = {10.1109/LRA.2022.3160077},
	abstract = {Collisions between rigid bodies are impulsive events, meaning they create near-instantaneous, finite discontinuities in the velocity state of the dynamic system. If velocity is assumed to be continuous, that requirement may come into conflict with the desire for bodies to not interpenetrate, or with Coulomb friction (creating a condition known as the Painlevé paradox). This letter presents a contact-implicit framework for trajectory optimization using direct collocation that combines an unplanned impulsive contact model with an implicit, high-order numerical integration scheme. We demonstrate that this framework can resolve problems that other implicit formulations cannot, namely elastic or partially elastic collisions, and impacts without collision – the established resolution of the Painlevé paradox. We then evaluate its applicability to legged robotics problems and test its ability to discover the intricate sequence of varied collision types necessary to execute a skateboarding trick.},
	number = {2},
	journal = {IEEE Robotics and Automation Letters},
	author = {Shield, Stacey and Johnson, Aaron M. and Patel, Amir},
	month = apr,
	year = {2022},
	note = {Conference Name: IEEE Robotics and Automation Letters},
	keywords = {Belts, Force, Friction, Mathematical models, Multi-contact whole-body motion planning and control, Numerical models, optimization and optimal control, Robot kinematics, task and motion planning, Trajectory optimization},
	pages = {5779--5786},
	file = {IEEE Xplore Abstract Record:/Users/j.t.heinen/Zotero/storage/WRNB7RYJ/9739950.html:text/html;IEEE Xplore Full Text PDF:/Users/j.t.heinen/Zotero/storage/LV9STBV5/Shield e.a. - 2022 - Contact-Implicit Direct Collocation With a Discont.pdf:application/pdf},
}

